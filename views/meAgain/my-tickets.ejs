<!DOCTYPE html>
<html lang="en-US" dir="ltr">
<head>
  <%- include('../partials/header.ejs'); %>

  <script async type="text/javascript" src="/js/components/TicketCard.js"></script>
  <link rel="stylesheet" type="text/css" href="/css/search.css">
  <link rel="stylesheet" type="text/css" href="/css/me.css">
  <link rel="stylesheet" type="text/css" href="/css/my-apps.css"> <%# this is needed for the "you dont have tickets" stuff" %>
  <link rel="stylesheet" type="text/css" href="/css/meAgain.css">
  <script defer type="text/javascript" src="/js/me/my-tickets.js"></script>
  <script defer type="text/javascript" src="/js/meAgain/profile.js"></script>

</head>
<%- include('../partials/layout/top.ejs'); %>

<%- include('./partials/top.ejs')%>
<div class="grid-container me-section" style="padding:0">
  <div class="grid-x grid-margin-x">

    <div class="cell medium-12 box card profile-section">
      <h2> <i class="far fa-sync"></i> Open Tickets</h2>
      <div class="profile-section-content" id="tickets-open">
        <h3>My Open Tickets</h3>
      </div>
      <div class="profile-section-content" id="tickets-subscribed">
        <h3>Tickets I'm Subscribed To</h3>
      </div>
    </div>
    <div class="cell medium-12 grid-container me-section" style="padding:0;margin-bottom: 1em;">
      <h2 class="section-header">Quick Links</h2>
      <div class="grid-x grid-margin-x small-up-1 medium-up-2 large-up-2">
        <a href="/request" target="_blank" class="cell me-quicklink">Open a New Ticket</a>
        <a href="/feedback" target="_blank" class="cell me-quicklink">Provide Feedback</a>
      </div>
    </div>
    <div class="cell medium-12 box card profile-section">
      <h2> <i class="far fa-history"></i> Ticket History</h2>
      <div class="profile-section-content" id="tickets-history">
        <h3>My Closed Tickets</h3>
      </div>
    </div>
  
  </div>
  
</div>

<%- include('./partials/bottom.ejs'); %>

<%- include('../partials/layout/bottom.ejs'); %>
</html>