<%- include('../../../partials/components/toaster.ejs', { toasts }); %>
<div class="grid-container fluid">
  <div class="grid-container grid-x grid-margin-x" style="margin:auto;">
    <div class='cell  show-for-small-only'>
      <div style="font-size:8em;text-align:center;margin:.25em">
        <span class="fa-stack">
          <i class="fas fa-mobile-alt fa-stack-1x"></i>
          <i style="color:var(--color-pop)" class="fas fa-ban fa-stack-2x"></i>
        </span>
      </div>
      <p>Because this form requires extensive writing and text entry, for ease of use and best results, we recommend accessing the form from a computer.</p>
    </div>
    <div class='cell hide-for-small-only medium-4 large-3'>
      <% if (locals.user && user.id && locals.userAttributes) { %>
        <div class="user-info">
