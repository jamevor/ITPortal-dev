<%
var canAccessConsole = false;
if (locals.user) {
  if (typeof user.permissions === 'object') {
    for (let userPermission in user.permissions) {
      if (user.permissions[userPermission].level !== null) {
        canAccessConsole = true;
        break;
      }
    }
  }
  if (!canAccessConsole && Array.isArray(user.groups)) {
    canAccessConsole = user.groups.some(g => {
      if (typeof g.permissions === 'object') {
        for (let groupPermission in g.permissions) {
          if (g.permissions[groupPermission].level !== null) {
            return true;
          }
        }
      }
    });
  }
  if (!canAccessConsole && (user.isAdmin || user.isSuperUser)) {
    canAccessConsole = true;
  }
}
%>

<section class="user-sidebar" data-simplebar>
  <a href="/Me/Widgets" class="user-sidebar-configure">
    <i class="far fa-cog"><span class="show-for-sr">Customize Your Sidebar</span></i>
  </a>
  <div class="user-menu">
    <div class="user-menu-avatar" style="background-image:url('/img/ico-intro.svg')"></div>
    <div class="user-menu-name"><%- locals.greeting %></div>
    <div class="user-menu-links">
	  <a href="/Me/My-Dashboard"><i class="far fa-robot"></i> <span>My Dashboard</span></a>
	  <a href="/Me/My-TechFlex"><i class="custom-icon" style="height:1em;"><%- include('../components/tech-flex-svg.ejs'); %></i> <span>My TechFlex</span></a>
      <%# <a href="#"><i class="far fa-bell"></i> <span>Notifications</span></a> %>
      <% if (canAccessConsole) { %>
        <a href="/console"><i class="far fa-user-cog"></i> <span>Admin Console</span></a>
      <% } %>
      <a href="/logout" class="dark"><i class="far fa-sign-out-alt"></i> <span>Sign Out</span></a>
    </div>
  </div>

	<% if (locals.userSidebarWidgets === true) { %>
		<% if (locals.user && Array.isArray(user.widgets) && user.widgets.filter(w => w.userHasWidget.isSidebar).length) { %>
			<% for (const widget of user.widgets.filter(w => w.userHasWidget.isSidebar).sort((a, b) => a.userHasWidget.orderSidebar < b.userHasWidget.orderSidebar ? -1 : 1)) { %>
				<script src="<%- widget.scriptSrc %>" type="text/javascript" defer></script>
				<style><%- widget.styles %></style>
				<div class="user-widget <%= widget.containerId %>" id="<%= widget.containerId %>" data-widgetid="<%= widget.id %>">
					<div class="user-widget-header">
						<div class="user-widget-title"><i class="far <%= widget.icon %>"></i> <%- widget.title %></div>
						<% if (widget.link !== null) { %>
							<a href="<%- widget.link %>" target="<%= widget.target %>" class="user-widget-context-button"><%- widget.linkTitle %></a>
						<% } %>
					</div>
					<%# Stuff goes here via ajax calls %>
				</div>
			<% } %>
			<div class="switch tiny">
				<input class="switch-input" id="sidebar-drag-toggle" type="checkbox" name="sidebar-drag-toggle">
				<label class="switch-paddle" for="sidebar-drag-toggle">
					<span class="show-for-sr">Drag to Reorder</span>
					<span class="switch-active"><fa class="far fa-fw fa-lock-open-alt"></fa></span>
					<span class="switch-inactive"><fa class="far fa-fw fa-lock-alt"></fa></span>
				</label>
			</div>
		<% } else { %>
			<div class="user-widget empty">
				<p>You do not currently have any Widgets</p>
				<a href="/Me/Widgets" class="button"><i class="far fa-plus-circle"></i> Customize Your Sidebar</a>
			</div>
		<% } %>
  <% } else { %>
    <div class="user-widget empty standby">
      <p>You are currently editing your widgets</p>
      
      <style>
        .empty.standby{
          
        }
        .standby svg:hover .standby-intro{
          animation-name: rotateIntro; 
          animation-duration: 4s; 
          animation-iteration-count: infinite;
          animation-timing-function: linear;
          transform-origin: 175px 139.59px;
        }
        .standby svg .standby-text{
          transition: all .5s ease;
        }
        .standby svg:hover .standby-text .standby7{
          fill:var(--color-pop);
        }
        @keyframes rotateIntro {
            from {transform: rotate(0deg);}
            to {transform: rotate(360deg);}
        }
      </style>
