<style>
.pizza-tracker{
  display:flex;
  justify-content: space-between;
  align-content:middle;
  align-items: center;
}
.pizza-tracker ul.pizza-increment{
  display:flex;
  justify-content: space-evenly;
  margin:0 -1em 0 -1em;
  flex-grow:1; 
  background:#E0243C;
  padding:1em;
  z-index:1;
  border-bottom: 3px solid var(--color-body-headers);
  border-top: 3px solid var(--color-body-headers);
  box-shadow:0 3px 2px rgba(0, 0, 0, .3);
}

.pizza-tracker ul.pizza-increment li{
  display:inline-block;
  text-align: center;
}
.pizza-tracker ul.pizza-increment li a{
  border-radius:50%;
  background:var(--color-body);
  color:var(--color-body-headers);
  font-size:1.3em;
  height:2em;
  width:2em;
  display: flex;
  font-family:"Merriweather",serif;
  font-weight:600;
  justify-content: center;
  align-items: center;
  text-align:center;
  transition:all .5s ease;
  box-shadow: inset 1px -2px 2px rgba(0,0,0,.2);
}
.pizza-tracker ul.pizza-increment.loggedout li a{
  border-radius:0;
  background:transparent;
  height:auto;
  width:auto;
  box-shadow:none;
  letter-spacing: 4px;
  overflow: hidden;
  background: linear-gradient(90deg, transparent, var(--color-chrome), transparent);
  background-repeat: no-repeat;
  background-size: 80%;
  animation: textshimmer 4s linear infinite;
  background-clip: text;
  -webkit-background-clip: text;
  -webkit-text-fill-color: rgba(255, 255, 255, 0);
  display: inline-block;
}
.pizza-tracker ul.pizza-increment.loggedout li a:hover{
  background-clip: text;
  -webkit-background-clip: text;
  -webkit-text-fill-color: rgba(255, 255, 255, 0);
  box-shadow: none;
  color:var(--color-body);
}
.pizza-tracker ul.pizza-increment li.done a{
  background:#1BB0D4;
}
.pizza-tracker ul.pizza-increment li.done a:hover{
  background:#38c3e5;
  box-shadow: none;

}
.pizza-tracker ul.pizza-increment li.notyet a{
  background:var(--color-lane-subtler);
  color:#424242;
  cursor:not-allowed;
  box-shadow: none;
}
.pizza-tracker ul.pizza-increment li.notyet a:hover{
  background:var(--color-lane-subtler);
  cursor:not-allowed;
  box-shadow: none;
}
.pizza-tracker ul.pizza-increment li a:hover{
  background:var(--color-lane);
  box-shadow: inset 3px -4px 6px rgba(0,0,0,.5);
}
.pizza-prize{
  z-index:1;
  width:150px;
  height:150px;
  display:flex;
  justify-content: center;
  align-items: center;
}
.pizza-prize.done svg{
  filter:drop-shadow( 0 3px 2px rgba(0, 0, 0, .3));
}
.pizza-prize svg{
  height:200px;
  margin:-1.25em;
  filter: grayscale(100%) drop-shadow( 0 3px 2px rgba(0, 0, 0, .3));
}
.pizza-prize svg text{
  font-family:"Segoe Print",serif;
}
.pizza-logo{
  background:#E0243C;
  border-radius:50%;
  z-index:50;
  transition: all .5s ease;
}
.pizza-logo svg{
  fill:var(--color-body-headers);
  height:200px;
  margin: -29px -33px -35px -21px;
  filter: drop-shadow( 0 3px 2px rgba(0, 0, 0, .3))
}
.pizza-logo:hover{
  background:#1BB0D4;
  transform: rotateZ(-15deg);
}

.pizza-prize:not(.done):hover {
  animation: shake 0.82s cubic-bezier(.36,.07,.19,.97) both;
  transform: translate3d(0, 0, 0);
  backface-visibility: hidden;
  perspective: 1000px;
}

@keyframes shake {
  10%, 90% {
    transform: translate3d(-1px, 0, 0);
  }
  
  20%, 80% {
    transform: translate3d(2px, 0, 0);
  }

  30%, 50%, 70% {
    transform: translate3d(-4px, 0, 0);
  }

  40%, 60% {
    transform: translate3d(4px, 0, 0);
  }
}
@keyframes textshimmer {
  0% {
    background-position: -500%;
  }
  100% {
    background-position: 500%;
  }
}
@media screen and (max-width: 39.9375em) {
  .pizza-tracker{
    flex-direction:column;
  }
  .pizza-logo, .pizza-prize{
    display:block;
  }
  .pizza-logo{
    margin-bottom:-4em;
  }
  .pizza-prize{
    transform: scale(.5); 
    margin-top:-4em;
  }
  .pizza-tracker ul.pizza-increment{
    border: 3px solid var(--color-body-headers);
    border-radius:1em;  
    width:100%;
    flex-wrap: wrap;
    padding-top: 4em;
  }
  .pizza-tracker ul.pizza-increment li a{
    font-size:1em;
    height:3em;
    width:3em;
    margin: 1em auto;
  }
  .pizza-tracker ul.pizza-increment li{
    flex: 1 0 21%;
  }
}
</style>

